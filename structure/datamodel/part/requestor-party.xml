<Element xmlns="urn:fdc:difi.no:2017:vefa:structure-1">
    <Term>cac:RequestorParty</Term>
    <Name>Requestor party</Name>
    <Description>The party requesting the evidence, normally the contracting authority in a procurement procedure</Description>
    
<!--    <Property key="doc">structure/docs/contracting-party.adoc</Property>-->
    
    <Element>
        <Term> cbc:EndpointID</Term>
        <Name>Requestor electronic address</Name>
        <Description></Description>
        <Attribute>
            <Term>schemeID</Term>
            <Name>Identifier qualifier</Name>
            <Reference type="CODE_LIST">ICD</Reference>
            <Value type="FIXED">NO:ORGNR</Value>
        </Attribute>
        <Value type="EXAMPLE">991825827</Value>
        
    </Element>

        <Element>
            <Term>cac:PartyIdentification</Term>
            <Name>Party identifier</Name>

            <Element>
                <Term>cbc:ID</Term>
                <Name>Requestor identifier</Name>
                <Description>The identifier of the requesting party, as it is legally registered</Description>

                <Attribute>
                    <Term>schemeID</Term>
                    <Name>Identifier qualifier</Name>
                    <Reference type="CODE_LIST">ICD</Reference>
                    <Value type="FIXED">NO:ORGNR</Value>
                </Attribute>

                <Value type="EXAMPLE">991825827</Value>
            </Element>
        </Element>

        <Element cardinality="0..1">
            <Term>cac:PartyName</Term>
            <Name>Party name</Name>

            <Element>
                <Term>cbc:Name</Term>
                <Name>Requestor name</Name>
                <Description>The name of the requestor</Description>
                <Value type="EXAMPLE">Difi</Value>
            </Element>
        </Element>

  


    </Element>

