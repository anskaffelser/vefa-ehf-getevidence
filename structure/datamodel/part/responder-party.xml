<Element xmlns="urn:fdc:difi.no:2017:vefa:structure-1">
    <Term>cac:ResponderParty</Term>
    <Name>Responder party</Name>
    <Description>The party responding to the request for evidence, Altinn</Description>
    
<!--    <Property key="doc">structure/docs/contracting-party.adoc</Property>-->
    
    <Element>
        <Term> cbc:EndpointID</Term>
        <Name>Responder electronic address</Name>
        <Description></Description>
        <Attribute>
            <Term>schemeID</Term>
            <Name>Identifier qualifier</Name>
            <Reference type="CODE_LIST">ICD</Reference>
            <Value type="FIXED">NO:ORGNR</Value>
        </Attribute>
        <Value type="EXAMPLE">999888222</Value>
        
    </Element>

        <Element>
            <Term>cac:PartyIdentification</Term>
            <Name>Party identifier</Name>

            <Element>
                <Term>cbc:ID</Term>
                <Name>Responder identifier</Name>
                <Description>The identifier of the responding party, as it is legally registered</Description>

                <Attribute>
                    <Term>schemeID</Term>
                    <Name>Identifier qualifier</Name>
                    <Reference type="CODE_LIST">ICD</Reference>
                    <Value type="FIXED">NO:ORGNR</Value>
                </Attribute>

                <Value type="EXAMPLE">999888222</Value>
            </Element>
        </Element>

        <Element cardinality="0..1">
            <Term>cac:PartyName</Term>
            <Name>Party name</Name>

            <Element>
                <Term>cbc:Name</Term>
                <Name>Responder name</Name>
                <Description>The name of the responder</Description>
                <Value type="EXAMPLE">Altinn</Value>
            </Element>
        </Element>

  


    </Element>

